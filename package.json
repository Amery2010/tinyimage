{
  "name": "tinyimage",
  "version": "1.0.0",
  "description": "用于图片压缩",
  "main": "build/main.js",
  "repository": "git@github.com:Amery2010/tinyimage.git",
  "author": "Amery <amery@xiangfa.org>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "tsc && electron ./build/main.js",
    "watch": "tsc -w",
    "lint": "eslint ./src --fix --ext .ts",
    "build": "rimraf ./build && tsc"
  },
  "dependencies": {
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "menubar": "^9.0.0"
  },
  "devDependencies": {
    "@types/imagemin": "^7.0.0",
    "@types/imagemin-gifsicle": "^5.2.0",
    "@types/imagemin-mozjpeg": "^8.0.0",
    "@types/imagemin-optipng": "^5.2.0",
    "@types/imagemin-pngquant": "^7.0.0",
    "@types/imagemin-svgo": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "electron": "^9.0.0",
    "eslint": "^7.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "overrides": [
      {
        "files": [
          "*.ts"
        ],
        "parser": "@typescript-eslint/parser",
        "extends": [
          "plugin:@typescript-eslint/recommended"
        ],
        "plugins": [
          "@typescript-eslint"
        ],
        "rules": {
          "@typescript-eslint/no-explicit-any": 0
        }
      }
    ]
  }
}

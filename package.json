{
  "name": "tinyimage",
  "version": "0.7.0",
  "author": "Amery<amery@xiangfa.org>",
  "description": "A tool to compress pictures.",
  "homepage": ".",
  "main": "build/electron.js",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "electron": "tsc -p electron/tsconfig.json && electron build/electron.js",
    "build": "react-scripts build && tsc -p electron/tsconfig.json",
    "test": "react-scripts test",
    "pack": "electron-builder --dir",
    "release": "electron-builder"
  },
  "build": {
    "icon": "build/logo512.png",
    "asar": false,
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  },
  "dependencies": {
    "electron-util": "^0.14.2",
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.0",
    "imagemin-svgo": "^8.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/imagemin": "^7.0.0",
    "@types/imagemin-gifsicle": "^5.2.0",
    "@types/imagemin-mozjpeg": "^8.0.0",
    "@types/imagemin-pngquant": "^7.0.0",
    "@types/imagemin-svgo": "^7.0.0",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "electron": "^9.0.3",
    "electron-builder": "^22.7.0",
    "react-scripts": "^3.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
